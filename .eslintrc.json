{
  "parser": "babel-eslint",
  "extends": ["plugin:prettier/recommended", "eslint:recommended"],
  "plugins": ["react"],
  "rules": {
    "react/prop-types": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1
  },
  "env": {
    "es6": true,
    "node": true,
    "jest": true,
    "browser": true
  }
}
